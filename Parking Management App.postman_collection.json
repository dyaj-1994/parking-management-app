	"item": [
		{
			"name": "Auth with Google",
			"item": [
				{
					"name": "Generate OAuth Token",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"if (pm.request.auth && pm.request.auth.type === 'oauth2') {\r",
									"    const token = pm.request.auth.oauth2.get('access_token');\r",
									"    if (token) {\r",
									"        pm.environment.set('access_token', token);\r",
									"    } else {\r",
									"        console.log(\"different key\");\r",
									"    }\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"auth": {
							"type": "oauth2",
							"oauth2": [
								{
									"key": "scope",
									"value": "email profile",
									"type": "string"
								},
								{
									"key": "clientSecret",
									"value": "{{google-client-secret}}",
									"type": "string"
								},
								{
									"key": "clientId",
									"value": "{{google-client-id}}",
									"type": "string"
								},
								{
									"key": "accessTokenUrl",
									"value": "https://oauth2.googleapis.com/token",
									"type": "string"
								},
								{
									"key": "authUrl",
									"value": "https://accounts.google.com/o/oauth2/v2/auth",
									"type": "string"
								},
								{
									"key": "useBrowser",
									"value": true,
									"type": "boolean"
								},
								{
									"key": "tokenName",
									"value": "Google Auth",
									"type": "string"
								},
								{
									"key": "addTokenTo",
									"value": "header",
									"type": "string"
								}
							]
						},
						"method": "GET",
						"header": []
					},
					"response": []
				},
				{
					"name": "Validate Access Token",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "https://www.googleapis.com/oauth2/v3/tokeninfo?access_token=YOUR_ACCESS_TOKEN",
							"protocol": "https",
							"host": [
								"www",
								"googleapis",
								"com"
							],
							"path": [
								"oauth2",
								"v3",
								"tokeninfo"
							],
							"query": [
								{
									"key": "access_token",
									"value": "YOUR_ACCESS_TOKEN"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "Validate Id Token",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "https://oauth2.googleapis.com/tokeninfo?id_token=YOUR_ID_TOKEN",
							"protocol": "https",
							"host": [
								"oauth2",
								"googleapis",
								"com"
							],
							"path": [
								"tokeninfo"
							],
							"query": [
								{
									"key": "id_token",
									"value": "YOUR_ID_TOKEN"
								}
							]
						}
					},
					"response": []
				}
			]
		},
